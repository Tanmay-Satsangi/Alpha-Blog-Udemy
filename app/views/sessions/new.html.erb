<h1 class="text-center mt-4"> Log in for alpha blog </h1>

# After session it will go to the login_path
<%= form_with(scope: :session, url: login_path, local: true, data: { turbo: false }) do |f| %>
    <p>
      <%= f.label :email %> <br/>
      <%= f.email_field :email %>
    </p>

    <p> 
      <%= f.label :password %> <br/>
      <%= f.password_field :password %>
    </p>
  
    <p>
      <%= f.submit "Log in" %>
    </p>
  <% end %>
